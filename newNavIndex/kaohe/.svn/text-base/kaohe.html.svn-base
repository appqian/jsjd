        function ds_fixed(data){
            
        	switch (data){
        	
        	case "N": 
        		data = "否"; 
        		break;
        	case "Y":
        		data = "是";
        		break;
        	case '1':
        		data = "草稿";
        		break;
        	case '2':
        		data = "审核中";
        		break;
        	case '3':
        		data = "审核通过";
        		break;
        	case '4':
        		data = "驳回";
        		break;
        	}
        	return data;
        }  


    }
    

