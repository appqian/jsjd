@charset "utf-8";
/* CSS Document */
* { margin: 0; padding: 0; }
body { min-width: 960px; background: #fff; font-size: 11pt; font-family: "微软雅黑", "黑体", "宋体"; color: #000000;}
div, p, span { line-height: 25px; }
dl, dt, dd, ul, ol, li, h1, h2, h3, h4, h5, h6 { text-align: left; font-size: 11pt; font-weight: normal; line-height: 25px;}
li { list-style: none; }
a { text-decoration: none; color: #000000; }
img { border: 0; vertical-align: middle; }
.fengge { clear: both; display: block; font-size: 1px; line-height: 1px; height: 0.1px; margin: 0; padding: 0; }
/*以上为常用参数*/
.wu_left { position: fixed; left: 1%; top: 20px; width: 14%; min-width: 135px; border: 1px solid #ECECEC; overflow: auto; z-index: 11;height:100%; }
/* 超链接 */
.st_tree { width: 94%; margin: 10px auto; overflow: auto; }
.st_tree a { text-decoration: none; }
/* 鼠标经过的超链接 */
.st_tree a:hover { color: #f33; text-decoration: underline; }
/* 菜单 */
.st_tree ul { padding: 0 0 0 18px; margin: 0; overflow: visible; }
/* 菜单项 */
.st_tree ul li { width: 90px; font-size: 13px; color: #222; line-height: 18px; cursor: pointer; list-style: none; background-repeat: no-repeat; padding: 0 0 3px 0; white-space: nowrap; overflow: visible; }
/* 子菜单 */
.st_tree ul li ul { }
/* 子菜单项 */
.st_tree ul li ul li { }
/* 子菜单的父节点 */
.st_tree .folder { list-style-image: url(../img/st_icon.png); background: url(../img/st_folder.gif); background-repeat: no-repeat; padding: 0 0 0 20px; }
/* 展开的父节点 */
.st_tree .open { list-style-image: url(../img/st_icon_open.png); background: url(../img/st_folder_open.gif); background-repeat: no-repeat; padding: 0 0 0 20px; }
.wu_top { position: fixed; top: 0; left: 0; width: 100%; min-width: 960px; height: auto; z-index: 10; }
.wu_top0 { float: left; width: 100%; height: 20px; background: #ffffff; }
.wu_top1 { float: right; display: inline; width: 83%; min-width: 796.8px; height: auto; margin: 0 1% 0 0; border: 1px solid #ECECEC; line-height: 36px; text-align: center; font-size: 13pt; background: #ffffff; }
.wu_top1 span { float: left; display: inline; padding: 0 10px; height: 28px; line-height: 28px; margin: 5px 0 5px 10px; text-align: center; font-size: 11pt; color: #ffffff; background: #2fa9cc; border: 0; border-radius: 5px; cursor: pointer; vertical-align: middle; }
.wu_top1 ol { float: left; width: 100%; height: auto; overflow: visible; }
.wu_top1 li { float: left; height: 28px; margin: 5px 15px 0 5px; position: relative; overflow: visible; }
.wu_top1 h2 { float: left; height: 28px; line-height: 28px; color: #666666; text-decoration: underline; cursor: pointer; }
.wu_top1 h2:hover { color: #177eda; }
.wu_top1 h3 { position: absolute; top: 28px; left: 0; z-index: 3500; width: 200px; height: auto; padding: 5px 10px; background: #000000; border-radius: 5px; z-index: 3000; color: #ffffff; display: none; }
.wu_top1b:hover { background: #145A96; }
.wu_top1a { float: right; display: inline; width: 200px; height: 26px; margin: 5px 10px 5px 0; line-height: 26px; font-size: 11pt; text-indent: 5px; border: 1px solid #177eda; }
.wu_top1b { float: right; display: inline; width: 60px; height: 28px; margin: 5px 10px 5px 0; text-align: center; font-size: 11pt; color: #ffffff; background: #177eda; border: 0; cursor: pointer; vertical-align: middle; }
.wu_top1b:hover { background: #145A96; }
.choose { outline: none; display: none; }
.select_box { position: relative; float: right; width: 100px; margin: 5px 10px 5px 0; }
.select_showbox { width: 100%; height: 26px; border: 1px solid #177eda; background: url(../img/icon.jpg) no-repeat right center; font-size: 11pt; line-height: 26px; text-indent: 5px; }
.select_option { position: absolute; left: 0; top: 28px; width: 100%; border: 1px solid #2fa9cc; border-top: none; display: none; background: #ffffff; }
.select_option li { float: left; width: 100%; height: 26px; line-height: 26px; text-indent: 5px; font-size: 11pt; margin: 0; padding: 0; }
.select_option li.selected { background-color: #F3F3F3; color: #999; }
.select_option li.hover { background: #2fa9cc; color: #fff; }
.wu_main { float: right; display: inline; width: 83%;min-height:535px; margin: 0 1% 20px 0; border: 1px solid #ECECEC; border-top: none; }
.wu_main1 { width: 98%; height: auto; min-height: 200px; margin: 10px auto; }
.wu_main1 p { width: 100%; height: 30px; line-height: 30px; text-align: center; font-size: 13pt; color: #333333; }
.wu_tab1 { width: 100%; margin: 15px auto; background: #ffffff; jerry: expression(cellSpacing="1"); border-collapse: separate; border-spacing: 1px; border: 1px solid #cccccc; }
.wu_tab1 thead td { height: 40px; padding: 0 10px; background: #f2f2f2; text-align: center; vertical-align: middle; line-height: normal; color: #177eda; font-size: 12pt; }
.wu_tab1 tbody td { height: 40px; padding: 0 10px; text-align: center; vertical-align: middle; line-height: normal; }
.wu_tab1 tbody tr:nth-of-type(even) { background: #f2f2f2; }
.wu_tab1 tbody tr:hover { background: #e4e4e4; }
.wu_tab1 td[rowspan]{background:#f2f2f2;}



.zhexian { width: 120px; cursor: pointer; }
.zhexian p img { height: 50%; margin: 10px auto; }
.lineDiv { position: fixed; z-index: 4000; border: 1px solid #cccccc; background: #ffffff; display: none; }
.drsMoveHandle { height: 40px; background: #177eda; cursor: move; }
.drsMoveHandle span { float: right; width: 30px; height: 40px; cursor: pointer; background: url(../img/wj_close.jpg) right center no-repeat; }
.linecontent {width: 700px; height: 300px; margin: 10px auto; overflow: hidden; text-align: center; }

//左侧tree
#tree li{
	font-size: 14px!important;
}
ul.ztree {
 
    border: 1px solid #617775;
   
    width: 100%;
   
    overflow-x: auto;
    overflow-y: scroll;
}
.ztree {
    margin: 0;
    padding: 5px;
    color: #333;
}

//分页
.pagination a {
    text-decoration: none;
	border: 1px solid #AAE;
	color: #15B;
}

.pagination a, .pagination span {
    display: inline-block;
    padding: 0.1em 0.4em;
    margin-right: 5px;
	margin-bottom: 5px;
}

.pagination .current {
    background: #26B;
    color: #fff;
	border: 1px solid #AAE;
}

.pagination .current.prev, .pagination .current.next{
	color:#999;
	border-color:#999;
	background:#fff;
}
.select{
	padding-top:3px;
	height: 42px;
    line-height: 38px;
    text-align: center;
    background-color: #f5f5f5;
    border-top: 1px solid #eee;
    border-bottom: 1px solid #ddd;
}
.link{
	cursor: pointer;
}