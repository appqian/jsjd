.select_option { border: 1px solid #2fa9cc; border-top: none; display: none; background-color: #ffffff; }
.select_option li { text-indent: 5px; font-size: 11pt; }
.select_option li.selected { background-color: #F3F3F3; color: #999; }
.select_option li.hover { background: #2fa9cc; color: #fff; }

.clearfix:after, .clearfix:before {
    content: "";
    display: table;
    overflow: hidden;
    height: 0;
    width: 0;
}

.clearfix:after {
    clear: both;
}

.clearfix {
    zoom: 1;
}

a:focus {
    outline-offset: -2px;
}

a:hover,
a:active {
    outline: 0;
    color: red;
}

a {
    color: #000;
    text-decoration: none;
    font-size:12px;
    width:100%;
    text-align:center
}
table td{
    font-size:12px;
    text-align:center;
}

a:hover {
    color: red;
    text-decoration: underline;
}

